{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "Controllers/**",
          "middleware/**",
          "Models/**",
          "routes/**",
          "utils/**",
          "uploads/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}